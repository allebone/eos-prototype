A,MAAA,KAAA,EAAA,EAHA;AAIA,MAAA,iBAAA,EAAA;AAJA,KAAA;AAMA,GArBA;AAsBA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA;AACA,aAAA,KAAA,MAAA,CAAA,IAAA,GAAA,WAAA,EAAA;AACA,KAJA;AAKA,IAAA,gBALA,8BAKA;AAAA;;AAAA,UACA,QADA,GACA,IADA,CACA,QADA,EAGA;;AACA,UAAA,OAAA,GAAA,KAAA,OAAA,CAAA,MAAA,CAAA,UAAA,GAAA;AAAA,eAAA,KAAA,CAAA,KAAA,CAAA,OAAA,CAAA,GAAA,MAAA,CAAA,CAAA;AAAA,OAAA,CAAA;;AACA,UAAA,QAAA,EAAA;AACA;AACA,eAAA,OAAA,CAAA,MAAA,CAAA,UAAA,GAAA;AAAA,iBAAA,GAAA,CAAA,WAAA,GAAA,OAAA,CAAA,QAAA,IAAA,CAAA,CAAA;AAAA,SAAA,CAAA;AACA,OARA,CAUA;;;AACA,aAAA,OAAA;AACA,KAjBA;AAkBA,IAAA,UAlBA,wBAkBA;AACA,UAAA,KAAA,QAAA,IAAA,KAAA,gBAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,eAAA,iDAAA;AACA;;AACA,aAAA,EAAA;AACA;AAvBA,GAtBA;AA+CA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,+BACA;AAAA,UAAA,MAAA,QAAA,MAAA;AAAA,UAAA,MAAA,QAAA,MAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA;AACA,WAAA,MAAA,GAAA,EAAA;AACA;AAJA;AA/CA,CAAA","sourcesContent":["<template>\n  <b-card-code\n    title=\"Advanced custom rendering usage\"\n  >\n    <b-card-text>\n      <span>In situations where the </span>\n      <code>inputHandlers</code>\n      <span>\n        will not work with your custom input, or if you need greater control over tag creation, you can take advantage\n        of the additional properties provided via the default slot's scope.\n      </span>\n    </b-card-text>\n\n    <div>\n      <b-form-group label=\"Tagged input using dropdown\">\n        <b-form-tags\n          v-model=\"value\"\n          no-outer-focus\n        >\n          <template #default=\"{ tags, disabled, addTag, removeTag }\">\n            <ul\n              v-if=\"tags.length > 0\"\n              class=\"list-inline d-inline-block mb-1\"\n            >\n              <li\n                v-for=\"tag in tags\"\n                :key=\"tag\"\n                class=\"list-inline-item\"\n              >\n                <b-form-tag\n                  :title=\"tag\"\n                  :disabled=\"disabled\"\n                  variant=\"primary\"\n                  @remove=\"removeTag(tag)\"\n                >\n                  {{ tag }}\n                </b-form-tag>\n              </li>\n            </ul>\n\n            <b-dropdown\n              size=\"sm\"\n              variant=\"outline-secondary\"\n              block\n              menu-class=\"w-100\"\n            >\n              <template #button-content>\n                <feather-icon icon=\"LinkIcon\" />\n                <span class=\"ml-25\">Choose tags</span>\n              </template>\n              <b-dropdown-form @submit.stop.prevent=\"() => {}\">\n                <b-form-group\n                  label-for=\"tag-search-input\"\n                  label=\"Search tags\"\n                  label-cols-md=\"auto\"\n                  class=\"mb-0\"\n                  label-size=\"sm\"\n                  :description=\"searchDesc\"\n                  :disabled=\"disabled\"\n                >\n                  <b-form-input\n                    id=\"tag-search-input\"\n                    v-model=\"search\"\n                    type=\"search\"\n                    size=\"sm\"\n                    autocomplete=\"off\"\n                  />\n                </b-form-group>\n              </b-dropdown-form>\n              <b-dropdown-divider />\n              <b-dropdown-item\n                v-for=\"option in availableOptions\"\n                :key=\"option\"\n                @click=\"onOptionClick({ option, addTag })\"\n              >\n                {{ option }}\n              </b-dropdown-item>\n              <b-dropdown-text v-if=\"availableOptions.length === 0\">\n                There are no tags available to select\n              </b-dropdown-text>\n            </b-dropdown>\n          </template>\n        </b-form-tags>\n      </b-form-group>\n    </div>\n\n    <template #code>\n      {{ codeAdvanceCustom }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code'\nimport {\n  BFormTags,\n  BFormGroup,\n  BDropdown,\n  BDropdownForm,\n  BFormInput,\n  BDropdownDivider,\n  BDropdownItem,\n  BFormTag,\n  BDropdownText,\n  BCardText,\n} from 'bootstrap-vue'\nimport { codeAdvanceCustom } from './code'\n\nexport default {\n  components: {\n    BCardCode,\n    BFormTags,\n    BFormGroup,\n    BDropdown,\n    BDropdownForm,\n    BFormInput,\n    BDropdownDivider,\n    BCardText,\n    BDropdownItem,\n    BFormTag,\n    BDropdownText,\n  },\n  data() {\n    return {\n      options: ['Apple', 'Orange', 'Banana', 'Lime', 'Peach', 'Chocolate', 'Strawberry'],\n      search: '',\n      value: [],\n      codeAdvanceCustom,\n    }\n  },\n  computed: {\n    criteria() {\n      // Compute the search criteria\n      return this.search.trim().toLowerCase()\n    },\n    availableOptions() {\n      const { criteria } = this\n\n      // Filter out already selected options\n      const options = this.options.filter(opt => this.value.indexOf(opt) === -1)\n      if (criteria) {\n        // Show only options that match criteria\n        return options.filter(opt => opt.toLowerCase().indexOf(criteria) > -1)\n      }\n\n      // Show all options available\n      return options\n    },\n    searchDesc() {\n      if (this.criteria && this.availableOptions.length === 0) {\n        return 'There are no tags matching your search criteria'\n      }\n      return ''\n    },\n  },\n  methods: {\n    onOptionClick({ option, addTag }) {\n      addTag(option)\n      this.search = ''\n    },\n  },\n}\n</script>\n"],"sourceRoot":"src/views/forms/form-element/form-tag"}]}