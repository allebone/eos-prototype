r\"\n      rows=\"3\"\n      :state=\"value.length <= maxChar\"\n      class=\"char-textarea\"\n      :class=\"value.length >= maxChar ? 'text-danger' : ''\"\n    />\n    <small\n      class=\"textarea-counter-value float-right\"\n      :class=\"value.length >= maxChar ? 'bg-danger' : ''\"\n    >\n      <span class=\"char-count\">{{ value.length }}</span> / {{ maxChar }}\n    </small>\n\n    <template #code>\n      {{ codeCounter }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code'\nimport { BFormTextarea, BCardText } from 'bootstrap-vue'\nimport { codeCounter } from './code'\n\nexport default {\n  components: {\n    BCardCode,\n    BCardText,\n    BFormTextarea,\n  },\n  data() {\n    return {\n      value: '',\n      maxChar: 20,\n      codeCounter,\n    }\n  },\n}\n</script>\n"],"sourceRoot":"src/views/forms/form-element/form-textarea"}]}