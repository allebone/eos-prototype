AA;AACA,UAAA,KAAA,EAAA,qBADA;AAEA,UAAA,IAAA,EAAA;AAFA;AAFA,OAAA;AAOA,KAlBA;AAmBA,IAAA,MAnBA,oBAmBA;AACA,WAAA,MAAA,CAAA;AACA,QAAA,SAAA,EAAA,qBADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,KAAA,EAAA,oBADA;AAEA,UAAA,IAAA,EAAA;AAFA;AAFA,OAAA;AAOA,KA3BA;AA4BA,IAAA,QA5BA,sBA4BA;AACA,WAAA,MAAA,CAAA;AACA,QAAA,SAAA,EAAA,qBADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,KAAA,EAAA,sBADA;AAEA,UAAA,IAAA,EAAA;AAFA;AAFA,OAAA;AAOA;AApCA;AAjBA,CAAA","sourcesContent":["<template>\n  <b-card-code title=\"Tooltip Events\">\n    <div class=\"demo-inline-spacing\">\n\n      <!-- show button -->\n      <b-button\n        id=\"tooltip-button-show-event\"\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\n        variant=\"outline-primary\"\n      >\n        Show Event Tooltip\n      </b-button>\n      <!-- tooltip -->\n      <b-tooltip\n        ref=\"tooltip-show\"\n        target=\"tooltip-button-show-event\"\n        triggers=\"click\"\n        @show=\"onShow\"\n      >\n        tooltip!\n      </b-tooltip>\n\n      <!-- button shown -->\n      <b-button\n        id=\"tooltip-button-shown-event\"\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\n        variant=\"outline-primary\"\n      >\n        Shown Event Tooltip\n      </b-button>\n      <!-- tooltip -->\n      <b-tooltip\n        ref=\"tooltip-shown\"\n        target=\"tooltip-button-shown-event\"\n        triggers=\"click\"\n        @shown=\"onShown\"\n      >\n        tooltip!\n      </b-tooltip>\n\n      <!-- button hide -->\n      <b-button\n        id=\"tooltip-button-hide-event\"\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\n        variant=\"outline-primary\"\n      >\n        Hide Event Tooltip\n      </b-button>\n      <!-- tooltip -->\n      <b-tooltip\n        ref=\"tooltip-hide\"\n        target=\"tooltip-button-hide-event\"\n        triggers=\"click\"\n        @hide=\"onHide\"\n      >\n        tooltip!\n      </b-tooltip>\n\n      <!-- button hidden -->\n      <b-button\n        id=\"tooltip-button-hidden-event\"\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\n        variant=\"outline-primary\"\n        class=\"mb-1 mb-xl-0\"\n      >\n        Hidden Event Tooltip\n      </b-button>\n\n      <!-- tooltip -->\n      <b-tooltip\n        ref=\"tooltip-hidden\"\n        target=\"tooltip-button-hidden-event\"\n        triggers=\"click\"\n        @hidden=\"onHidden\"\n      >\n        tooltip!\n      </b-tooltip>\n    </div>\n\n    <template #code>\n      {{ codeEvent }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code'\nimport {\n  VBTooltip, BButton, BTooltip,\n} from 'bootstrap-vue'\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\nimport Ripple from 'vue-ripple-directive'\nimport { codeEvent } from './code'\n\nexport default {\n  components: {\n    BCardCode,\n    BButton,\n    BTooltip,\n    // eslint-disable-next-line vue/no-unused-components\n    ToastificationContent,\n  },\n  directives: {\n    'b-tooltip': VBTooltip,\n    Ripple,\n  },\n  data() {\n    return {\n      codeEvent,\n    }\n  },\n  methods: {\n    onShow() {\n      this.$toast({\n        component: ToastificationContent,\n        props: {\n          title: 'Show Event Tooltip',\n          icon: 'BellIcon',\n        },\n      })\n    },\n    onShown() {\n      this.$toast({\n        component: ToastificationContent,\n        props: {\n          title: 'Shown Event Tooltip',\n          icon: 'BellIcon',\n        },\n      })\n    },\n    onHide() {\n      this.$toast({\n        component: ToastificationContent,\n        props: {\n          title: 'Hide Event Tooltip',\n          icon: 'BellIcon',\n        },\n      })\n    },\n    onHidden() {\n      this.$toast({\n        component: ToastificationContent,\n        props: {\n          title: 'Hidden Event Tooltip',\n          icon: 'BellIcon',\n        },\n      })\n    },\n  },\n}\n</script>\n"],"sourceRoot":"src/views/components/tooltip"}]}