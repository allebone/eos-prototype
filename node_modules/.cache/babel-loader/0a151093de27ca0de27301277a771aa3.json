s.$toast({\n          component: ToastificationContent,\n          props: {\n            title: 'Can not copy!',\n            icon: 'BellIcon',\n          },\n        })\n      })\n    },\n  },\n}\n</script>\n"],"sourceRoot":"src/views/extensions/clipboard"}]}