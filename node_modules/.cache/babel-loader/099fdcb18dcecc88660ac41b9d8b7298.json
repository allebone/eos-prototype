in the date picker, specify a function reference to the </span>\n      <code>date-disabled-fn</code>\n      <span> prop.</span>\n    </b-card-text>\n\n    <b-form-datepicker\n      v-model=\"value\"\n      :date-disabled-fn=\"dateDisabled\"\n      locale=\"en\"\n    />\n\n    <template #code>\n      {{ codeDisableDate }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code'\nimport { BFormDatepicker, BCardText } from 'bootstrap-vue'\nimport { codeDisableDate } from './code'\n\nexport default {\n  components: {\n    BCardCode,\n    BCardText,\n    BFormDatepicker,\n  },\n  data() {\n    return {\n      value: '',\n      codeDisableDate,\n    }\n  },\n  methods: {\n    dateDisabled(ymd, date) {\n      // Disable weekends (Sunday = `0`, Saturday = `6`) and\n      // disable days that fall on the 13th of the month\n      const weekday = date.getDay()\n      const day = date.getDate()\n\n      // Return `true` if the date should be disabled\n      return weekday === 0 || weekday === 6 || day === 13\n    },\n  },\n}\n</script>\n"],"sourceRoot":"src/views/forms/form-element/form-datepicker"}]}