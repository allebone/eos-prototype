i size\"\n  >\n    <b-card-text>\n      <span>For different sizes of select2, Use classes like </span>\n      <code>.select-size-sm</code>\n      <span> & </span>\n      <code>.select-size-lg</code>\n      <span> for Large, small & Extra Small Select respectively.</span>\n    </b-card-text>\n\n    <b-row>\n\n      <!-- Small -->\n      <b-col cols=\"12\">\n        <b-form-group>\n          <v-select\n            v-model=\"selected\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            label=\"title\"\n            :options=\"option\"\n            class=\"select-size-sm\"\n            multiple\n          />\n        </b-form-group>\n      </b-col>\n\n      <!-- Medium -->\n      <b-col cols=\"12\">\n        <b-form-group>\n          <v-select\n            v-model=\"selected\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            label=\"title\"\n            :options=\"option\"\n            multiple\n          />\n        </b-form-group>\n      </b-col>\n\n      <!-- Large -->\n      <b-col cols=\"12\">\n        <b-form-group>\n          <v-select\n            v-model=\"selected\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            label=\"title\"\n            :options=\"option\"\n            class=\"select-size-lg\"\n            multiple\n          />\n        </b-form-group>\n      </b-col>\n    </b-row>\n\n    <template #code>\n      {{ codeVueMultipleSize }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport {\n  BRow, BCol, BFormGroup, BCardText,\n} from 'bootstrap-vue'\n\nimport BCardCode from '@core/components/b-card-code'\nimport vSelect from 'vue-select'\nimport { codeVueMultipleSize } from './code'\n\nexport default {\n  components: {\n    BRow,\n    BCol,\n    BFormGroup,\n    BCardText,\n    BCardCode,\n    vSelect,\n  },\n  data() {\n    return {\n      selected: { title: 'Square' },\n      codeVueMultipleSize,\n      option: [{ title: 'Square' }, { title: 'Rectangle' }, { title: 'Rombo' }, { title: 'Romboid' }],\n    }\n  },\n}\n</script>\n"],"sourceRoot":"src/views/forms/form-element/vue-select"}]}