A;AACA,QAAA,MAAA,GAAA,CAAA,SAAA,EAAA,WAAA,EAAA,SAAA,EAAA,MAAA,EAAA,SAAA,EAAA,QAAA,EAAA,OAAA,EAAA,MAAA,CAAA,CAFA,CAIA;;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,EAAA,GAAA,GAAA,MAAA,CAAA,MAAA,EAAA,CAAA,GAAA,GAAA,EAAA,CAAA,EAAA,EAAA;AACA,MAAA,YAAA,CAAA,MAAA,CAAA,CAAA,CAAA,CAAA,GAAA,SAAA,aAAA,MAAA,CAAA,CAAA,CAAA,GAAA,QAAA,CAAA,eAAA,CAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,KAPA,CASA;;;AACA,QAAA,WAAA,GAAA,CAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,CAAA,CAVA,CAYA;;AACA,SAAA,IAAA,EAAA,GAAA,CAAA,EAAA,IAAA,GAAA,WAAA,CAAA,MAAA,EAAA,EAAA,GAAA,IAAA,EAAA,EAAA,EAAA,EAAA;AACA,MAAA,iBAAA,CAAA,WAAA,CAAA,EAAA,CAAA,CAAA,GAAA,MAAA,CAAA,SAAA,wBAAA,WAAA,CAAA,EAAA,CAAA,GAAA,QAAA,CAAA,eAAA,CAAA,CAAA,KAAA,CAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA,CAAA,CAAA;AACA,KAfA,CAiBA;;;AAjBA,QAkBA,KAlBA,GAkBA,YAAA,CAAA,MAlBA,CAkBA,KAlBA;AAmBA,IAAA,QAAA,CAAA,eAAA,CAAA,YAAA,CAAA,KAAA,EAAA,KAAA,GAAA,KAAA,GAAA,KAAA;AACA,GAzCA;AA0CA,EAAA,KA1CA,mBA0CA;AAAA,wBACA,YAAA,EADA;AAAA,QACA,IADA,iBACA,IADA;AAAA,QACA,WADA,iBACA,WADA;;AAAA,QAEA,iBAFA,GAEA,YAAA,CAAA,MAFA,CAEA,iBAFA,EAIA;;AACA,QAAA,IAAA,CAAA,KAAA,KAAA,MAAA,EAAA,QAAA,CAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,aAAA,EALA,CAOA;AACA;AACA;;AACA,IAAA,YAAA,CAAA;AACA,MAAA,eAAA,EAAA,IADA;AAEA,MAAA,YAAA,EAAA,KAFA;AAGA,MAAA,WAAA,EAAA,KAHA;AAIA,MAAA,IAAA,EAAA,KAJA;AAKA,MAAA,OAAA,EAAA,IALA;AAMA,MAAA,UAAA,EAAA;AANA,KAAA,CAAA,CAVA,CAmBA;;AACA,IAAA,KAAA,CAAA,MAAA,CAAA,yBAAA,EAAA,MAAA,CAAA,UAAA;;AApBA,yBAqBA,aAAA,EArBA;AAAA,QAqBA,WArBA,kBAqBA,KArBA;;AAsBA,IAAA,KAAA,CAAA,WAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,yBAAA,EAAA,GAAA;AACA,KAFA,CAAA;AAIA,WAAA;AACA,MAAA,WAAA,EAAA,WADA;AAEA,MAAA,iBAAA,EAAA;AAFA,KAAA;AAIA;AAxEA,CAAA","sourcesContent":["<template>\n  <div\n    id=\"app\"\n    class=\"h-100\"\n    :class=\"[skinClasses]\"\n  >\n    <component :is=\"layout\">\n      <router-view />\n    </component>\n\n    <scroll-to-top v-if=\"enableScrollToTop\" />\n  </div>\n</template>\n\n<script>\nimport ScrollToTop from '@core/components/scroll-to-top/ScrollToTop.vue'\n\n// This will be populated in `beforeCreate` hook\nimport { $themeColors, $themeBreakpoints, $themeConfig } from '@themeConfig'\nimport { provideToast } from 'vue-toastification/composition'\nimport { watch } from '@vue/composition-api'\nimport useAppConfig from '@core/app-config/useAppConfig'\n\nimport { useWindowSize, useCssVar } from '@vueuse/core'\n\nimport store from '@/store'\n\nconst LayoutVertical = () => import('@/layouts/vertical/LayoutVertical.vue')\nconst LayoutHorizontal = () => import('@/layouts/horizontal/LayoutHorizontal.vue')\nconst LayoutFull = () => import('@/layouts/full/LayoutFull.vue')\n\nexport default {\n  components: {\n\n    // Layouts\n    LayoutHorizontal,\n    LayoutVertical,\n    LayoutFull,\n\n    ScrollToTop,\n  },\n  // ! We can move this computed: layout & contentLayoutType once we get to use Vue 3\n  // Currently, router.currentRoute is not reactive and doesn't trigger any change\n  computed: {\n    layout() {\n      if (this.$route.meta.layout === 'full') return 'layout-full'\n      return `layout-${this.contentLayoutType}`\n    },\n    contentLayoutType() {\n      return this.$store.state.appConfig.layout.type\n    },\n  },\n  beforeCreate() {\n    // Set colors in theme\n    const colors = ['primary', 'secondary', 'success', 'info', 'warning', 'danger', 'light', 'dark']\n\n    // eslint-disable-next-line no-plusplus\n    for (let i = 0, len = colors.length; i < len; i++) {\n      $themeColors[colors[i]] = useCssVar(`--${colors[i]}`, document.documentElement).value.trim()\n    }\n\n    // Set Theme Breakpoints\n    const breakpoints = ['xs', 'sm', 'md', 'lg', 'xl']\n\n    // eslint-disable-next-line no-plusplus\n    for (let i = 0, len = breakpoints.length; i < len; i++) {\n      $themeBreakpoints[breakpoints[i]] = Number(useCssVar(`--breakpoint-${breakpoints[i]}`, document.documentElement).value.slice(0, -2))\n    }\n\n    // Set RTL\n    const { isRTL } = $themeConfig.layout\n    document.documentElement.setAttribute('dir', isRTL ? 'rtl' : 'ltr')\n  },\n  setup() {\n    const { skin, skinClasses } = useAppConfig()\n    const { enableScrollToTop } = $themeConfig.layout\n\n    // If skin is dark when initialized => Add class to body\n    if (skin.value === 'dark') document.body.classList.add('dark-layout')\n\n    // Provide toast for Composition API usage\n    // This for those apps/components which uses composition API\n    // Demos will still use Options API for ease\n    provideToast({\n      hideProgressBar: true,\n      closeOnClick: false,\n      closeButton: false,\n      icon: false,\n      timeout: 3000,\n      transition: 'Vue-Toastification__fade',\n    })\n\n    // Set Window Width in store\n    store.commit('app/UPDATE_WINDOW_WIDTH', window.innerWidth)\n    const { width: windowWidth } = useWindowSize()\n    watch(windowWidth, val => {\n      store.commit('app/UPDATE_WINDOW_WIDTH', val)\n    })\n\n    return {\n      skinClasses,\n      enableScrollToTop,\n    }\n  },\n}\n</script>\n"],"sourceRoot":"src"}]}